{
  "name": "cocotola-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint-old": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint": "run-p lint:*",
    "lint:tsc": "tsc --noEmit",
    "lint:prettier": "prettier --config .prettierrc 'src/**/*.{ts,tsx}' --check",
    "lint:eslint": "eslint src/**/*.{ts,tsx} --ext .ts,.tsx",
    "fix": "run-s fix:*",
    "fix:eslint": "eslint src/**/*.{ts,tsx} --ext .ts,.tsx --fix",
    "fix:prettier": "prettier --config .prettierrc 'src/**/*.{ts,tsx}' --write",
    "test": "vitest run --coverage"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.1.1",
    "@reduxjs/toolkit": "^2.0.1",
    "@types/howler": "^2.2.11",
    "axios": "^1.6.2",
    "clsx": "^2.1.0",
    "framer-motion": "^10.17.0",
    "howler": "^2.2.4",
    "jwt-decode": "^3.1.2",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.4",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.21.1",
    "react-use-audio-player": "^2.2.0",
    "redux": "^5.0.1",
    "use-sound": "^4.0.1",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/coverage-v8": "^1.1.3",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-vitest": "^0.3.20",
    "happy-dom": "^12.10.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-tsconfig-paths": "^4.2.2",
    "vitest": "^1.1.3"
  },
  "volta": {
    "node": "20.10.0",
    "npm": "10.2.5"
  }
}
